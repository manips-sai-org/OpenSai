<?xml version='1.0' encoding='utf-8'?>
<robot name="dog">

    <link name="BASE_FRAME">
		<inertial>
			<mass value="0.001"/>
			<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
		</inertial>
	</link>
	<link name="VIRTUAL_1">
		<inertial>
			<mass value="0.001"/>
			<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
		</inertial>
	</link>
	<link name="DOG">
		<inertial>
			<mass value="5.0"/>
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
		<visual>
            <geometry>
				<cylinder radius="0.05" length="0.1"/>
			</geometry>
        </visual>
		<collision>
            <geometry>
				<cylinder radius="0.05" length="0.1"/>
			</geometry>
        </collision>
	</link>


    <joint name="prismatic1" type="prismatic">
		<parent link="BASE_FRAME"/>
		<child link="VIRTUAL_1"/>
		<origin xyz="0 0 0" rpy="0 -0 0 "/>
		<axis xyz="1 0 0"/>
		<limit lower="-5" upper="5" effort="10000" velocity="1000" />
	</joint>
	<joint name="prismatic2" type="prismatic">	
		<parent link="VIRTUAL_1"/>
		<child link="DOG"/>
		<origin xyz="0 0 0" rpy="0 -0 0 "/>
		<axis xyz="0 1 0"/>
		<limit lower="-5" upper="5" effort="10000" velocity="1000" />
	</joint>
   
</robot>
